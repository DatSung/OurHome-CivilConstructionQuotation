<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"]</title>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/SWP391.CHCQS.OurHomeWeb.styles.css" asp-append-version="true" />

	<!-- Custom fonts for this template-->
	<link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css" asp-append-version="true">
	<link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
		  rel="stylesheet" asp-append-version="true">

	<!-- Custom styles for this template-->
	<link href="~/css/sb-admin-2.min.css" rel="stylesheet" asp-append-version="true">

	<!-- Link toast Notification -->
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" asp-append-version="true">
	<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.css" asp-append-version="true" />

	<!-- Link datatables css -->
	<link rel="stylesheet" href="//cdn.datatables.net/1.13.7/css/jquery.dataTables.min.css" asp-append-version="true" />

	<!--Link sweet alert-->
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>
<body id="page-top">
	<!-- Page Wrapper -->
	<div id="wrapper">

		<!-- Sidebar -->
		<ul class="navbar-nav bg-main sidebar sidebar-dark accordion" id="accordionSidebar">

			<!-- Sidebar - Brand -->
			<a asp-area="Seller" asp-controller="Home" asp-action="Index" style="background-color: white" class="sidebar-brand d-flex align-items-center justify-content-center">
				<div class="sidebar-brand-icon">
					<img src="~/images/logo.png" width="100%"></img>
				</div>
			</a>

			<!-- Divider -->
			<hr class="sidebar-divider my-0">

			<!-- Nav Item - Customer Request -->

			<li class="nav-item active">
				<a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseTwo"
				   aria-expanded="true" aria-controls="collapseTwo">
					<i class="fas fa-fw fa-cog"></i>
					<span>Yêu cầu</span>
				</a>
				<div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
					<div class="bg-white py-2 collapse-inner rounded">
						<a class="collapse-item" asp-area="Seller" asp-controller="Home" asp-action="Index">Đang chờ</a>
						<a class="collapse-item" asp-area="Seller" asp-controller="Request" asp-action="ViewRequestCompleted">Hoàn thành</a>
						<a class="collapse-item" asp-area="Seller" asp-controller="Request" asp-action="ViewRequestRejected">Từ chối</a>
					</div>
				</div>
			</li>
			<hr class="sidebar-divider my-0">

			<li class="nav-item active">
				<a class="nav-link" asp-area="Seller" asp-controller="Quotation" asp-action="ViewQuotation">
					<i class="fas fa-fw fa-tachometer-alt"></i>
					<span>Xem báo giá đã gửi</span>
				</a>
			</li>

			
			<!-- Sidebar Toggler (Sidebar) -->
			<div class="text-center d-none d-md-inline">
				<button class="rounded-circle border-0" id="sidebarToggle"></button>
			</div>

		</ul>
		<!-- End of Sidebar -->
		<!-- Content Wrapper -->
		<div id="content-wrapper" class="d-flex flex-column">

			<!-- Main Content -->
			<div id="content">
				<!-- Topbar -->
				<nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

					<!-- Sidebar Toggle (Topbar) -->
					<button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
						<i class="fa fa-bars text-main"></i>
					</button>

					<!-- Topbar Navbar -->
					<div class="navbar-nav" style="justify-content:end; width:100%; margin-right: 1em ">

						<!-- Nav Item - User Information -->
							<partial name="_LoginPartial"></partial>

					</div>

				</nav>
				<!-- End of Topbar -->
				<!-- Begin Page Content -->
				<div class="container-fluid">
					<partial name="_Notification" />
					@RenderBody()

				</div>
				<!-- /.container-fluid -->

			</div>
			<!-- End of Main Content -->

		</div>
		<!-- End of Content Wrapper -->

	</div>
	<!-- End of Page Wrapper -->





	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	<!-- Bootstrap core JavaScript-->
	<script src="~/vendor/jquery/jquery.min.js"></script>
	<script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

	<!-- Core plugin JavaScript-->
	<script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

	<!-- Custom scripts for all pages-->
	<script src="~/js/sb-admin-2.min.js"></script>

	<!-- Page level plugins -->
	<script src="~/vendor/chart.js/Chart.min.js"></script>

	<!-- Page level custom scripts -->
	<script src="~/js/demo/chart-area-demo.js"></script>
	<script src="~/js/demo/chart-pie-demo.js"></script>
	<script src="https://cdn.tiny.cloud/1/9v3f6801xkjbo5g8uijy1kuncu1ltgp0khqtlsn6d9oulwdj/tinymce/6/tinymce.min.js" referrerpolicy="origin" asp-append-version="true"></script>

	<!-- Datatable scripts -->
	<script src="//cdn.datatables.net/1.13.7/js/jquery.dataTables.min.js" asp-append-version="true"></script>

	<!-- SignalR -->
	<script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>
	<script src="~/js/signalr/CustomQuotationNotification.js"></script>

	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
