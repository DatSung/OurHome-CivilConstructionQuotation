@{
    ViewData["Title"] = "Customer's Request";
}
<div class="card shadow border-0 my-4">
    <div class="card-header bg-main ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">Yêu cầu đang xử lí của khách hàng</h2>
            </div>
        </div>
    </div>

    <div class="card-body p-4">

        <div class="table-responsive">
            <table id="tblRequest" class="table-bordered table-striped text-black" style="width:100%">
                <thead>
                    <tr>
                        <th>
                            Mã
                        </th>
                        <th>
                            Ngày tạo
                        </th>
                        <th>
                            Tên khách hàng
                        </th>
                        <th>
                            Giới tính
                        </th>
                        <th>
                            SĐT
                        </th>
                        <th>
                            Email
                        </th>
                        <th>
                            Loại công trình
                        </th>
                        <th>
                            Diện tich
                        </th>
                        <th>
                            Vị trí
                        </th>
                        <th>
                            Trạng thái
                        </th>
                        <th>
                            Mô tả
                        </th>
                        <th>
                            Hành động
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>



@section Scripts {
    <script src="~/js/datatables/sellerDTB/RequestDTB.js"></script>
}
