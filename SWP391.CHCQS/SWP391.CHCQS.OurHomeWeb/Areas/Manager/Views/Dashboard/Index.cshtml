@{
    ViewData["Title"] = "Quotation Statistic";
}
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Biểu đồ thanh</h6>
    </div>
    <div class="">
        <select  id="yearSelect" class="form-select" aria-label="Default select example">
        </select>
        <div class="chart-bar">
            <canvas id="myQuoteBarChart"></canvas>
        </div>
    </div>
</div>
	<div class="card-body p-3">
		<div class="table-responsive">
			<table id="tblQuotation" class="table-bordered table-striped text-black" style="width:100%">
				<thead>
					<tr>
						<th>
							Mã
						</th>
						<th>
							Ngày tạo
						</th>
						<th>
							Diện tích 
						</th>
						<th>
							Vị trí
						</th>
						<th>
							Mô tả
						</th>
						<th>
							Trạng thái
						</th>
						
					</tr>
				</thead>
			</table>
		</div>
	</div>
</div>


@section Scripts {
    <script src="~/js/charts/QuoteStatisticChart.js"></script>
    <script src="~/js/event_load.js"></script>
}
      

